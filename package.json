{
  "name": "joyus-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "webpack --mode=development --watch",
    "dev:rust": "cargo watch -w src -w public/index.html -w public/assets -x run",
    "dev": "concurrently -k -n WEB,RUST -c auto \"npm:dev:web\" \"npm:dev:rust\"",
    "build": "webpack --mode=production"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.1",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^12.0.2"
  }
}
